{
    "name": "matter-survey/app",
    "description": "The matter survey app",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "doctrine/dbal": "^4.4",
        "doctrine/doctrine-bundle": "^2.18",
        "doctrine/doctrine-fixtures-bundle": "^4.3",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.5",
        "nelmio/cors-bundle": "^2.4",
        "symfony/asset-mapper": "^7.4",
        "symfony/cache": "^7.0",
        "symfony/console": "^7.3",
        "symfony/dotenv": "^7.3",
        "symfony/flex": "^2.10",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-client": "^7.3",
        "symfony/monolog-bundle": "^3.10",
        "symfony/rate-limiter": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/stimulus-bundle": "^2.31",
        "symfony/string": "^7.3",
        "symfony/translation": "^8.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/ux-chartjs": "^2.31",
        "symfony/ux-turbo": "^2.31",
        "symfony/validator": "^7.3",
        "symfony/yaml": "^7.3"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.4",
        "friendsofphp/php-cs-fixer": "^3.91",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^12.4",
        "symfony/browser-kit": "^7.3",
        "symfony/css-selector": "^7.3",
        "symfony/debug-bundle": "^7.3",
        "symfony/maker-bundle": "^1.50",
        "symfony/web-profiler-bundle": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
